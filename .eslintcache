  value={query}\n            onChange={(e) => updateInput(e.target.value)}\n            onKeyDown={(e) => hanldeKeyDown(e.key)}\n          />\n          <SearchIcon\n            className=\"input__searchIcon\"\n            onClick={() => searchFunc()}\n          />\n        </div>\n        <div className=\"main__tweetsList\">\n          {data.map((tweet, index) => (\n            <Tweet data={tweet} key={index} />\n          ))}\n          {/* <Tweet data={data} />\n          <Tweet data={data} /> */}\n        </div>\n      </div>\n      <div className=\"right\">\n        <div className=\"right__trending\">\n          <div className=\"trendingTable\">\n            <div className=\"trendingTable__row\">\n              <p>Trends for you</p>\n            </div>\n            <div className=\"trendingTable__row\">\n              <p onClick={() => searchHashtag(\"coding\")}>#Coding</p>\n            </div>\n            <div className=\"trendingTable__row\">\n              <p onClick={() => searchHashtag(\"javascript\")}>#JavaScript</p>\n            </div>\n            <div className=\"trendingTable__row\">\n              <p onClick={() => searchHashtag(\"python\")}>#Python</p>\n            </div>\n            <div className=\"trendingTable__row\"></div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\zachary.b.kula\\Documents\\Training\\MERN\\PWJ\\Twitter Clone\\pwj-twitter-clone-app-master\\twitter-clone\\src\\reportWebVitals.js",[],"C:\\Users\\zachary.b.kula\\Documents\\Training\\MERN\\PWJ\\Twitter Clone\\pwj-twitter-clone-app-master\\twitter-clone\\src\\Tweet.js",["28"],"C:\\Users\\zachary.b.kula\\Documents\\Training\\MERN\\PWJ\\Twitter Clone\\pwj-twitter-clone-app-master\\twitter-clone\\src\\exercise\\js\\index.js",["29","30","31","32","33","34","35"],{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":1,"column":8,"nodeType":"42","messageId":"43"